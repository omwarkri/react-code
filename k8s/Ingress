apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
name: react-app-ingress
namespace: react-prod
annotations:
kubernetes.io/ingress.class: "nginx"
spec:
rules:
- host: example.com # replace with your domain or omit for default
http:
paths:
- path: /
pathType: Prefix
backend:
service:
name: react-app-service
port:
number: 80